{
  "compilerOptions": {
    "outDir": "dist",
    "module": "CommonJS",
    "target": "es5",
    "lib": ["es6", "dom", "DOM.Iterable"],
    "sourceMap": true,
    "allowJs": true,
    "jsx": "react",
    "moduleResolution": "node",
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "importsNotUsedAsValues": "preserve",
    "skipLibCheck": true
  }
}
